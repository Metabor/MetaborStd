{
    "name": "metabor/metabor-std",
    "description": "Collection of maximal independent PHP Interfaces",
    "keywords": ["interfaces", "api", "standard"],
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Tischlinger",
            "email": "oliver.tischlinger@metabor.de"
        }
    ],
    "require": {
        "php": ">=5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "suggest": {
        "metabor/statemachine": "Working implementation of the statemachine interfaces"
    },
    "autoload": {
        "psr-0": {
        	"MetaborStd\\": "src/"
        }
    },
    "prefer-stable":true,
    "minimum-stability":"dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.2.x-dev"
        }
    }
}
